<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/rl_output"
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="wrap_content">
<TextView
    style="@style/item_textview"
    android:id="@+id/tv_title"
    android:text="单号"

    android:layout_width="80dp"
   />
    <TextView
        style="@style/item_textview"
        android:id="@+id/tv_id"
        android:textSize="12sp"
       android:layout_toRightOf="@id/tv_title"
        android:text="FJJZ0000000"

        android:layout_width="wrap_content"
        />
    <TextView
        android:textSize="12sp"
        android:layout_marginRight="30dp"
        android:layout_alignParentRight="true"
        android:text="2018/11/12 9:54"
        style="@style/item_textview"
        android:layout_width="wrap_content"
        />
    <TextView
        android:id="@+id/tv_projectrtitle"
        android:layout_width="80dp"
        android:layout_below="@id/tv_title"
        android:text="项目编号"
        style="@style/item_textview"
        />
    <TextView
        android:id="@+id/tv_explaintitle"
        android:layout_width="100dp"
        android:layout_below="@id/tv_projectrtitle"
        android:text="现场拆除说明"
        style="@style/item_textview"
        />
    <TextView
        android:id="@+id/tv_companytitle"
        android:layout_width="80dp"
        android:layout_below="@id/tv_explain"
        android:text="施工单位"
        style="@style/item_textview"

        />
    <TextView
        android:layout_below="@id/tv_id"
        style="@style/item_textview"
        android:id="@+id/tv_project"
        android:textSize="12sp"
        android:layout_toRightOf="@id/tv_title"
        android:text="260921701F7"
        android:layout_width="wrap_content"
        />
    <TextView

        android:layout_below="@id/tv_project"
        style="@style/item_textview"
        android:id="@+id/tv_explain"
        android:textSize="12sp"
        android:layout_toRightOf="@id/tv_explaintitle"
        android:text="永泰站"
        android:layout_width="wrap_content"
        />
    <TextView
        android:paddingTop="5dp"
        android:layout_below="@id/tv_explain"
        style="@style/item_textview"
        android:id="@+id/tv_company"
        android:textSize="12sp"
        android:layout_toRightOf="@id/tv_companytitle"
        android:text="上海神结环保科技股份有限公司（变电L）"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        />
    <ImageView
        android:layout_marginRight="10dp"
        android:layout_marginTop="30dp"
        android:layout_alignParentRight="true"
        android:background="@mipmap/ic_choose"
        android:layout_width="25dp"
        android:layout_height="25dp" />
    <TextView
         android:layout_marginRight="10dp"
        android:layout_alignParentRight="true"
        android:layout_below="@id/tv_explain"
        style="@style/item_textview"
        android:id="@+id/tv_progress"
        android:textSize="12sp"

        android:text="处理中"
        android:layout_width="wrap_content"
        />

</RelativeLayout>